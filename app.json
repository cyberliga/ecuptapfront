{
  "expo": {
    "name": "ecuptapfront",
    "slug": "ecuptapfront",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icons/EcupLogo.svg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/icons/EcupLogo.svg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.ecuptapfront"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icons/EcupLogo.svg",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-asset",
        {
          "assets": [
            "./assets/images/icons/EcoinsIcon.svg",
            "./assets/images/icons/EcupLogo.svg",
            "./assets/fonts/fonts/"
          ]
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Inter-Black.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "baseUrl": "/"
    },
    "sdkVersion": "51.0.0"
  }
}